{
  "name": "vianetz/pdf-generator",
  "description": "The vianetz PDF library that generates PDF documents.",
  "type": "library",
  "keywords": [
    "vianetz",
    "pdf"
  ],
  "homepage": "https://www.vianetz.com",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Christoph Massmann",
      "email": "cm@vianetz.com",
      "homepage": "https://www.vianetz.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@vianetz.com"
  },
  "autoload": {
    "psr-4": {
      "Vianetz\\Pdf\\": "src/"
    }
  },
  "require": {
    "php": "^5.6|^7|^8",
    "dompdf/dompdf": "^1.0",
    "setasign/fpdi": "^2.3",
    "setasign/fpdf": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "phpstan/phpstan": "^0.12.11",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "roave/backward-compatibility-check": "^5.0"
  },
  "suggest": {
    "vianetz/signaturportal-api": "Use this library to sign your generated PDF documents.",
    "zf1/zend-pdf": "Necessary if you want to use Zend_Pdf for merging. By default this package uses the fpdi merger."
  }
}
